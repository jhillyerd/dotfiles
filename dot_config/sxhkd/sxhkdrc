# Launchers
super + Return
  kitty

# super + e
#   $HOME/.scripts/dmenu-emoji.sh

super + b
  x-www-browser

super + p
  $HOME/.scripts/screenshot.sh

super + d
  rofi -show combi

super + v
  pavucontrol

super + Escape
  $HOME/.scripts/lock-screen.sh

XF86AudioRaiseVolume
  pactl set-sink-volume @DEFAULT_SINK@ +3%

XF86AudioLowerVolume
  pactl set-sink-volume @DEFAULT_SINK@ -3%
