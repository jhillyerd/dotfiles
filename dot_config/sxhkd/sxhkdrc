# Launchers
super + Return
  kitty

# super + e
#   $HOME/.scripts/dmenu-emoji.sh

super + b
  x-www-browser

super + c
  google-chrome-stable

super + p
  $HOME/.scripts/screenshot.sh

super + d
  rofi -show combi

super + v
  pavucontrol

super + slash
  thunar

super + Escape
  $HOME/.scripts/lock-screen.sh

XF86AudioRaiseVolume
  wpctl set-volume @DEFAULT_SINK@ 3%+

XF86AudioLowerVolume
  wpctl set-volume @DEFAULT_SINK@ 3%-
